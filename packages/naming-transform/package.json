{
  "name": "@tinyfe/naming-transform",
  "version": "0.0.7",
  "description": "Converting to different kind of naming style",
  "keywords": [
    "camel",
    "camel-cased",
    "pascal",
    "pascal-cased",
    "constant",
    "constant-cased",
    "underscore",
    "underscore-cased",
    "snake",
    "snake-cased",
    "kebab",
    "kebab-cased",
    "hyphen",
    "hyphen-cased",
    "cased",
    "case"
  ],
  "author": "Rain120 <1085131904@qq.com>",
  "homepage": "https://github.com/tinyfe/one-utils#readme",
  "license": "MIT",
  "main": "lib/index.js",
  "module": "es/index.js",
  "typings": "index.d.ts",
  "files": [
    "lib",
    "es"
  ],
  "publishConfig": {
    "assess": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tinyfe/one-utils.git"
  },
  "bugs": {
    "url": "https://github.com/tinyfe/one-utils/issues"
  },
  "scripts": {
    "test": "jest",
    "tsc": "tsc",
    "build": "rm -rf lib && rm -rf es && run-s --print-label \"build:**\"",
    "watch": "tsc -- -w",
    "build:es": "npm run tsc -- --outDir es --module es6",
    "build:cjs": "npm run tsc -- --outDir lib --module commonjs"
  },
  "devDependencies": {
    "@types/download": "^8.0.1",
    "@types/jest": "^26.0.20",
    "@types/node": "^16.4.13",
    "@types/rimraf": "^3.0.1",
    "download": "^8.0.0",
    "jest": "^27.3.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "typescript": "^4.4.4"
  },
  "gitHead": "1247fac4389d3807af69076ee041c63547ad5cb4"
}
